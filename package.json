{
  "name": "lyne-angular",
  "version": "19.0.0-beta.4",
  "scripts": {
    "ng": "ng",
    "start": "tsx scripts/serve.mts",
    "build:angular": "ng build angular",
    "postbuild:angular": "node --experimental-strip-types ./scripts/patch-package-version.mts",
    "build:angular-experimental": "ng build angular-experimental",
    "postbuild:angular-experimental": "node --experimental-strip-types ./scripts/patch-package-version.mts",
    "build:showcase": "ng build showcase",
    "build": "yarn -s build:angular && yarn -s build:angular-experimental && yarn -s build:showcase",
    "watch:angular": "ng build angular --watch --configuration development",
    "watch:angular-experimental": "ng build angular-experimental --watch --configuration development",
    "watch:showcase": "ng run showcase:storybook",
    "test": "echo \"TODO\"",
    "test:angular": "ng test angular",
    "format": "prettier \"**/*\" --write --ignore-unknown",
    "integrity": "yarn format",
    "lint": "ng lint",
    "lint fix": "ng lint --fix",
    "prepare": "husky"
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@angular/animations": "19.2.5",
    "@angular/cdk": "19.2.8",
    "@angular/common": "19.2.5",
    "@angular/compiler": "19.2.5",
    "@angular/core": "19.2.5",
    "@angular/forms": "19.2.5",
    "@angular/platform-browser": "19.2.5",
    "@angular/platform-browser-dynamic": "19.2.5",
    "@angular/router": "19.2.5",
    "@sbb-esta/lyne-design-tokens": "1.4.0",
    "@sbb-esta/lyne-elements": "2.6.0",
    "@sbb-esta/lyne-elements-experimental": "2.6.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.6",
    "@angular/cli": "19.2.6",
    "@angular/compiler-cli": "19.2.5",
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@compodoc/compodoc": "1.1.26",
    "@storybook/addon-a11y": "8.6.12",
    "@storybook/addon-actions": "8.6.12",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-links": "8.6.12",
    "@storybook/angular": "8.6.12",
    "@storybook/blocks": "8.6.12",
    "@storybook/manager-api": "8.6.12",
    "@storybook/preview-api": "8.6.12",
    "@storybook/test": "8.6.12",
    "@storybook/theming": "8.6.12",
    "@storybook/types": "8.6.12",
    "@types/jasmine": "5.1.7",
    "@types/node": "22.14.0",
    "@web/test-runner": "0.20.0",
    "@web/test-runner-commands": "0.9.0",
    "@web/test-runner-playwright": "0.11.0",
    "@web/test-runner-puppeteer": "0.17.0",
    "angular-eslint": "19.3.0",
    "concurrently": "9.1.2",
    "custom-elements-manifest": "2.1.0",
    "eslint": "9.24.0",
    "eslint-config-prettier": "10.1.1",
    "eslint-plugin-import-x": "4.10.2",
    "globals": "16.0.0",
    "husky": "9.1.7",
    "jasmine-core": "5.6.0",
    "lint-staged": "15.5.0",
    "ng-packagr": "19.2.1",
    "playwright": "1.51.1",
    "prettier": "3.5.3",
    "storybook": "8.6.12",
    "tsx": "4.19.3",
    "typescript": "5.8.3",
    "typescript-eslint": "8.29.1"
  },
  "prettier": {
    "singleQuote": true,
    "endOfLine": "lf",
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --no-ignore"
    ],
    "**/*": "prettier --write --ignore-unknown"
  }
}
