{
  "name": "lyne-angular",
  "version": "19.0.0-rc.0",
  "scripts": {
    "postinstall": "yarn playwright install",
    "ng": "ng",
    "start": "tsx scripts/serve.mts",
    "build:angular": "ng build angular",
    "postbuild:angular": "node --experimental-strip-types ./scripts/patch-package-version.mts",
    "build:angular-experimental": "ng build angular-experimental",
    "postbuild:angular-experimental": "node --experimental-strip-types ./scripts/patch-package-version.mts",
    "build:showcase": "ng build showcase",
    "build": "yarn -s build:angular && yarn -s build:angular-experimental && yarn -s build:showcase",
    "watch:angular": "ng build angular --watch --configuration development",
    "watch:angular-experimental": "ng build angular-experimental --watch --configuration development",
    "watch:showcase": "ng run showcase:storybook",
    "test": "npm-run-all --sequential test:*",
    "test:angular": "ng test angular",
    "test:angular-experimental": "ng test angular-experimental",
    "format": "prettier \"**/*\" --write --ignore-unknown",
    "integrity": "yarn format",
    "lint": "ng lint",
    "lint fix": "ng lint --fix",
    "prepare": "husky"
  },
  "private": true,
  "type": "module",
  "dependencies": {
    "@angular/animations": "19.2.10",
    "@angular/cdk": "19.2.16",
    "@angular/common": "19.2.10",
    "@angular/compiler": "19.2.10",
    "@angular/core": "19.2.10",
    "@angular/forms": "19.2.10",
    "@angular/platform-browser": "19.2.10",
    "@angular/platform-browser-dynamic": "19.2.10",
    "@angular/router": "19.2.10",
    "@sbb-esta/lyne-design-tokens": "1.5.1",
    "@sbb-esta/lyne-elements": "2.9.0",
    "@sbb-esta/lyne-elements-experimental": "2.9.0",
    "rxjs": "7.8.2",
    "tslib": "2.8.1",
    "zone.js": "0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "19.2.12",
    "@angular/cli": "19.2.12",
    "@angular/compiler-cli": "19.2.10",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@compodoc/compodoc": "1.1.26",
    "@storybook/addon-a11y": "8.6.12",
    "@storybook/addon-actions": "8.6.12",
    "@storybook/addon-essentials": "8.6.12",
    "@storybook/addon-interactions": "8.6.12",
    "@storybook/addon-links": "8.6.12",
    "@storybook/angular": "8.6.12",
    "@storybook/blocks": "8.6.12",
    "@storybook/manager-api": "8.6.12",
    "@storybook/preview-api": "8.6.12",
    "@storybook/test": "8.6.12",
    "@storybook/theming": "8.6.12",
    "@storybook/types": "8.6.12",
    "@types/jasmine": "5.1.8",
    "@types/node": "22.15.18",
    "angular-eslint": "19.4.0",
    "custom-elements-manifest": "2.1.0",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import-x": "4.11.1",
    "globals": "16.1.0",
    "husky": "9.1.7",
    "jasmine-core": "5.7.1",
    "karma": "6.4.4",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.1",
    "karma-firefox-launcher": "2.1.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "lint-staged": "16.0.0",
    "ng-packagr": "19.2.2",
    "npm-run-all2": "8.0.1",
    "playwright": "1.52.0",
    "prettier": "3.5.3",
    "storybook": "8.6.12",
    "tsx": "4.19.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.32.1"
  },
  "prettier": {
    "singleQuote": true,
    "endOfLine": "lf",
    "printWidth": 100
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix --no-ignore"
    ],
    "**/*": "prettier --write --ignore-unknown"
  }
}
